{
    "libraries": {
        "flock": "libflock-bedrock-module.so",
        "yokan": "libyokan-bedrock-module.so",
        "warabi": "libwarabi-bedrock-module.so",
        "mofka" : "libmofka-bedrock-module.so"
    },
    "providers": [
        {
            "name" : "my_group_manager",
            "type" : "flock",
            "provider_id" : 1,
            "config": {
                "bootstrap": "join",
                "file": "mofka.json",
                "group": {
                    "type": "centralized"
                }
            }
        },
        {
            "name": "my_metadata_provider_1",
            "provider_id": 3,
            "type": "yokan",
            "tags" : [ "mofka:metadata" ],
            "config" : {
                "database" : { "type": "map" }
            },
            "pool": "my_pool_1"
        },
        {
            "name": "my_metadata_provider_2",
            "provider_id": 4,
            "type": "yokan",
            "tags" : [ "mofka:metadata" ],
            "config" : {
                "database" : { "type": "map" }
            },
            "pool": "my_pool_2"
        },
        {
            "name": "my_data_provider_1",
            "provider_id": 5,
            "type": "warabi",
            "tags" : [ "mofka:data" ],
            "config" : {
                "target" : { "type": "memory" }
            },
            "pool": "my_pool_2"
        },
        {
            "name": "my_data_provider_2",
            "provider_id": 6,
            "type": "warabi",
            "tags" : [ "mofka:data" ],
            "config" : {
                "target" : { "type": "memory" }
            },
            "pool": "my_pool_2"
        }
    ],
    "margo": {
        "argobots": {
            "pools": [
                {
                    "name": "my_pool_1",
                    "access": "mpmc",
                    "type": "fifo_wait"
                },
                {
                    "name": "my_pool_2",
                    "access": "mpmc",
                    "type": "fifo_wait"
                }
            ],
            "xstreams": [
                {
                    "name": "my_es_1",
                    "scheduler": {
                        "type": "basic_wait",
                        "pools": ["my_pool_1", "my_pool_2"]
                    }
                },
                {
                    "name": "my_es_2",
                    "scheduler": {
                        "type": "basic_wait",
                        "pools": ["my_pool_2"]
                    }
                }
            ]
        }
    }
}
